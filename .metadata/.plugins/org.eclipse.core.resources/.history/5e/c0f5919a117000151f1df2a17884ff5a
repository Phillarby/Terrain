package TerrainBase;

import processing.core.*;

public class Scaffold {
	Cell _root;  //The root of the geometry tree

	void setup()
	{
	   
	  //Create root point
	  _root = new Cell(
	              0, //X Position
	              0, //Y Position
	              0, //Z Position
	              0, //Level (0 indicates root)
	              100, //X Size
	              100, //Y Size
	              100);//Z Size
	}

	//Logic for update cycle
	void update()
	{

	}

	//Logic for draw cycle
	void draw()
	{
	  _root.draw();
	}
}
